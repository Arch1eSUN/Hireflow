{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "./dist",
        "strict": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "src"
    ]
}
